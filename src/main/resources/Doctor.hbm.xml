<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.clinical.management.model.Doctor" table="doctor">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="username" type="string">
            <column name="username" length="32" not-null="true" unique="true"/>
        </property>
        <property name="firstName" type="string">
            <column name="firstName" length="32" not-null="true"/>
        </property>
        <property name="lastName" type="string">
            <column name="lastName" length="32" not-null="true"/>
        </property>
        <property name="password" type="string">
            <column name="password" length="302" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>